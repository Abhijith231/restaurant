<h1>Add Items</h1>
<%= form_for :item, url: item_index_path do |f| %>
  <p>
    <%= f.label :name%><br>
    <%= f.text_field :name%>
  </p>
  <p>
    <%= f.label :qty%><br>
    <%= f.text_field :qty%>
  </p>
  <p>
    <%= f.label :price%><br>
    <%= f.text_field :price%>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
